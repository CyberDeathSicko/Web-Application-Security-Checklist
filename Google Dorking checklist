# Google Dorks Search
# Customize Google Dorks for specific information

# Find Open Directories
site:yourtarget.com intitle:index.of

# Search for Sensitive Information
site:yourtarget.com filetype:pdf OR filetype:doc OR filetype:xls

# Look for Configuration Files
site:yourtarget.com ext:xml | ext:conf | ext:cnf | ext:reg | ext:inf | ext:rdp | ext:cfg | ext:txt

# Find Vulnerable Files
site:yourtarget.com ext:php intitle:index

# Search for Backup and Log Files
site:yourtarget.com ext:bkf | ext:bkp | ext:bak | ext:old | ext:backup | ext:log

# Locate Git Repositories
site:yourtarget.com inurl:.git

# Discover API Endpoints
site:yourtarget.com inurl:/api/

# Search for SQL Errors
site:yourtarget.com intext:"sql syntax near" | intext:"syntax error has occurred" | intext:"incorrect syntax near"

# Find Cross-Site Scripting (XSS) Vulnerabilities
site:yourtarget.com inurl:".php?parameter=" | inurl:".jsp?parameter=" | inurl:".aspx?parameter="

# Discover Subdomains
site:*.yourtarget.com -www

# Find Open Redirects
site:yourtarget.com inurl:redir | inurl:url | inurl:redirect

# Search for Login Pages
site:yourtarget.com inurl:login | inurl:signin | inurl:auth

# Look for Password Files
site:yourtarget.com ext:passwd | ext:password


# Discover Network Devices
site:yourtarget.com intitle:"Index of" inurl:devices.xml

# Search for Cross-Site Request Forgery (CSRF) Tokens
site:yourtarget.com inurl:csrf | inurl:token

# Find Publicly Exposed Documents
site:yourtarget.com ext:doc | ext:docx | ext:ppt | ext:pptx | ext:xls | ext:xlsx

# Look for Webcams
site:yourtarget.com inurl:view/view.shtml

# Discover Elasticsearch Instances
site:yourtarget.com port:"9200"

# Find MongoDB Instances
site:yourtarget.com port:"27017"

# Look for Jenkins Instances
site:yourtarget.com inurl:jenkins

# Search for Jira Instances
site:yourtarget.com inurl:jira

# Discover GitHub Repositories
site:github.com yourtarget

# Search for Misconfigured S3 Buckets
site:s3.amazonaws.com yourtarget

# Look for Open RDP Services
site:yourtarget.com port:"3389"

# Find Exposed AWS Keys
site:github.com "AWS_ACCESS_KEY_ID" "AWS_SECRET_ACCESS_KEY"

# Discover Cloudflare-Protected Domains
site:yourtarget.com "Always Onlineâ„¢"

# Search for WordPress Sites
site:yourtarget.com inurl:wp-content

# Look for Subdomain Takeover Vulnerabilities
site:yourtarget.com -www

# Discover Hidden Parameters in URLs
site:yourtarget.com inurl:"?id="

# Search for API Documentation
site:yourtarget.com inurl:swagger | inurl:apidocs

# Find OAuth Tokens
site:yourtarget.com "oauth_token"

# Look for Cross-Site Scripting (XSS) in URLs
site:yourtarget.com inurl:".php?parameter=" | inurl:".jsp?parameter=" | inurl:".aspx?parameter=" intext:"XSS"

# Search for Exposed Jenkins Files
site:yourtarget.com inurl:jenkinsfile

# Discover Elasticsearch Configuration Files
site:yourtarget.com intitle:"Elasticsearch" intext:"You Know, for Search"

# Look for Jira Configuration Files
site:yourtarget.com intitle:"Jira" intext:"System Information"

# Search for Sensitive Git Credentials
site:yourtarget.com inurl:.git ext:txt

# Discover Open Graph Protocol Information
site:yourtarget.com "og:description"

# Look for GitLab Instances
site:yourtarget.com inurl:gitlab

# Search for Sensitive Information in Error Messages
site:yourtarget.com intext:"Error Message" intext:"Password" | intext:"Error Message" intext:"Username"

# Find Exposed MongoDB Databases
site:yourtarget.com port:"27017"

# Discover Jenkins Configuration Files
site:yourtarget.com intitle:"Index of" inurl:jenkins

# Look for Exposed RDP Configurations
site:yourtarget.com intitle:"remote desktop web client"

# Search for Misconfigured ElasticSearch Instances
site:yourtarget.com intitle:"Index of" inurl:elasticsearch

# Discover Hidden Directories
site:yourtarget.com intitle:"Index of" | inurl:"/.git" | inurl:"/.svn" | inurl:"/.DS_Store"

# Find Specific File Types
site:yourtarget.com filetype:sql | filetype:json | filetype:xml | filetype:log

# Look for Open Jenkins Instances
site:yourtarget.com inurl:jenkins

# Search for AWS S3 Buckets with Public Access
site:yourtarget.com inurl:s3.amazonaws.com

# Discover Hidden Files
site:yourtarget.com intitle:"Index of" inurl:backup | inurl:logs | inurl:conf

# Look for MongoDB Configuration Files
site:yourtarget.com intitle:"Index of" inurl:mongodb

# Search for Exposed RDP Configurations
site:yourtarget.com intitle:"remote desktop web client"

# Discover Misconfigured Jenkins Instances
site:yourtarget.com inurl:jenkins

# Look for Sensitive Information in Git Commits
site:yourtarget.com "password" filetype:log | filetype:txt

# Search for Exposed Jupyter Notebooks
site:yourtarget.com inurl:tree | in
